<div class="container">
  <div class="card mt-4">
    <div class="card-body">
      <h3>New list</h3>
      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name, placeholder: 'eg. Thrillers' %>
        <%= f.input :photo, as: :file %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>


